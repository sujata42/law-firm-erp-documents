<%- include('../partials/html') %>

<head>
  <%- include('../partials/title-meta', { title: 'Document Center' }) %>
  <%- include('../partials/head-css') %>
</head>

<body>
<div class="wrapper">

  <%- include('../partials/menu') %>

  <div class="page-content">
  <div class="page-container">



      <div class="page-title-box mb-3">
  <h4 class="page-title">Document Center</h4>
</div>

      <!-- Filters -->
      <div class="card mb-3">
        <div class="card-body row g-2">
          <div class="col-md-3">
            <select class="form-control">
              <option>All Matters</option>
            </select>
          </div>
          <div class="col-md-2">
            <select class="form-control">
              <option>All Types</option>
              <option>Petition</option>
              <option>Affidavit</option>
            </select>
          </div>
          <div class="col-md-2">
            <input class="form-control" placeholder="Uploaded By">
          </div>
          <div class="col-md-3">
            <input type="date" class="form-control">
          </div>
          <div class="col-md-2">
            <button class="btn btn-primary w-100">Filter</button>
          </div>
        </div>
      </div>

      <!-- Table -->
      <div class="card">
        <div class="card-body">
          <table class="table table-bordered align-middle">
            <thead class="table-light">
              <tr>
                <th>Title</th>
                <th>Matter</th>
                <th>Type</th>
                <th>Version</th>
                <th>Updated</th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>Petition Draft</td>
                <td>Matter #101</td>
                <td>Petition</td>
                <td>v1</td>
                <td>Today</td>
                <td>
                  <button class="btn btn-sm btn-outline-primary">View</button>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>

    </div>

    <%- include('../partials/footer') %>
  </div>
</div>

<%- include('../partials/footer-scripts') %>
</body>
</html>
